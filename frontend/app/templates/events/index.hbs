<div class="list-group">
  {{#each model as |event|}}
    {{#link-to 'events.show' event class="list-group-item"}}
    <h4 class="list-group-item-heading">
      {{event.name}} <small>{{moment-format event.date 'LLLL'}}</small>
    </h4>
    <p>{{event.description}}</p>
    <strong>Biljettpris:</strong> {{event.price}}:-
  {{/link-to}}
{{/each}}
